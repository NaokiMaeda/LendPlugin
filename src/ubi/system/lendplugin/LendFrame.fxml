<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<GridPane gridLinesVisible="true" prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ubi.system.lendplugin.LendPluginController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="500.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
    <RowConstraints prefHeight="250.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label alignment="CENTER" prefHeight="100.0" prefWidth="300.0" text="ユーザー" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label alignment="CENTER" prefHeight="100.0" prefWidth="500.0" text="物品" textAlignment="CENTER" GridPane.columnIndex="1">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Button fx:id="executeButton" alignment="CENTER" mnemonicParsing="false" prefHeight="100.0" prefWidth="300.0" text="Enter" textAlignment="CENTER" GridPane.rowIndex="2">
         <font>
            <Font size="30.0" />
         </font>
      </Button>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="250.0" prefWidth="500.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <children>
            <ImageView fx:id="cameraImage" fitHeight="250.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
            <VBox prefHeight="250.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="QR結果">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <Label fx:id="qrResultLabel" alignment="CENTER" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label prefHeight="100.0" prefWidth="500.0" text="結果" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
         <children>
            <Label alignment="CENTER" prefHeight="50.0" prefWidth="300.0" text="Felica内容">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label fx:id="felicaResultLabel" alignment="CENTER" prefHeight="200.0" prefWidth="300.0" />
         </children>
      </VBox>
   </children>
</GridPane>
